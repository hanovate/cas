{
  "name": "hanovate/cas",
  "description": "Adds CAS support to Laravel 7.x",
  "keywords": [
    "CAS",
    "phpCAS",
    "SSO",
    "laravel",
    "laravel 7"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "subfission",
      "role": "Developer"
    },
    {
      "name": "Michael Han",
      "email": "mhan@hanovate.com",
      "role": "Developer"
    }
  ],
  "require": {
    "php": ">=7.2.0",
    "illuminate/support": "^6.0|^7.0|^8.0",
    "hanovate/phpcas": "dev-dev"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.3.3"
  },
  "autoload": {
    "psr-4": {
      "Hanovate\\Cas\\": "src/Hanovate/Cas/"
    },
    "files": [
      "src/Hanovate/Cas/functions.php"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "Hanovate\\Cas\\Tests\\": "tests"
    }
  },
  "minimum-stability": "stable",
  "extra": {
    "laravel": {
      "providers": [
        "Hanovate\\Cas\\CasServiceProvider"
      ],
      "aliases": {
        "Cas": "Hanovate\\Cas\\Facades\\Cas"
      }
    }
  }
}
